{
  "name": "swagapp",
  "version": "0.0.1",
  "private": false,
  "license": "MIT",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "yarn bootstrap && ts-node start-script.ts --test-mode && react-native start --reset-cache",
    "start:notest": "yarn bootstrap && ts-node start-script.ts && react-native start --reset-cache",
    "start:prod": "yarn bootstrap && ts-node start-script.ts --prod-server && react-native start --reset-cache",
    "test": "jest",
    "bootstrap": "yarn cc",
    "generate-icons": "npx icon-set-creator create ./assets/icon.jpg",
    "cc": "rimraf node_modules/react-native-config/ios/ReactNativeConfig/GeneratedDotEnv.m",
    "postinstall": "patch-package && yarn podinstall",
    "podinstall": "node -e \"if (process.platform === 'darwin') { require('child_process').execSync('rm ios/.xcode.env.local && plutil -convert xml1 ios/swagapp/Info.plist && npx pod-install && plutil -convert xml1 ios/swagapp/Info.plist', { stdio: 'inherit' }); }\""
  },
